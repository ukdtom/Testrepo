<?xml version="1.0" encoding="utf-8"?>
<plugins>
    <cachechk>PLEX_CACHECHK</cachechk>
    <item>
        <name>
            <![CDATA[ Plex Media Server ]]>
        </name>
        <internalName>PlexMediaServer</internalName>
        <changeLog>https://forums.plex.tv/t/plex-media-server/30447/last</changeLog>
        <category>Plex Inc</category>
        <type>Entertainment</type>
        <icon80>https://github.com/ukdtom/Testrepo/blob/main/PlexMediaServer_80x80.gif?raw=true</icon80>
        <icon100>https://github.com/ukdtom/Testrepo/blob/main/PlexMediaServer_100x100.gif?raw=true</icon100>
        <snapshot>https://github.com/ukdtom/Testrepo/blob/main/PlexQNAPScreenshot.png?raw=true</snapshot>
        <description><![CDATA[ Plex gives you one place to find and access all the media that matters to you. From personal media on your own server, to free and on-demand Movies & Shows, live TV, podcasts, and web shows, to streaming music, you can enjoy it all in one app, on any device. ]]></description>
        <fwVersion>4.3.0</fwVersion>
        <version>PLEX_VERSION</version>
        <publishedDate>PLEX_PUBDATE</publishedDate>
        <maintainer><![CDATA[ Plex build team ]]></maintainer>
        <developer><![CDATA[ Plex Inc ]]></developer>
        <forumLink><![CDATA[ https://forums.plex.tv ]]></forumLink>
        <language><![CDATA[English, 简体中文, 繁體中文, Czech, Dansk, Nederlands, Suomi, Français, Deutsch, Ελληνικά, Magyar, Italiano, 日本語, 한글, Norsk, Polski, Português, Român, Русский, Español, Svenska, ไทย, Turk dili]]></language>
        <bannerImg></bannerImg>
        <tutorialLink><![CDATA[ https://support.plex.tv/articles ]]></tutorialLink>
        <volumeSelect>3</volumeSelect>
        <platform>
          <!-- x86_64 -->
          <platformID>TS-NASX86</platformID>
          <platformExcl>TS-X75,TS-X75U</platformExcl>
          <location>PLEX_X86</location>
        </platform>
        <platform>
            <!-- Arm v8-->
            <platformID>TS-NASARM_64</platformID>
            <locationPLEX_ARM64</location>
        </platform>
        <platform>
          <platformID>QB-103</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-531P</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X31P2</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X31P3</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X31X</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X31XU</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X35</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <platform>
          <platformID>TS-X41</platformID>
          <location>PLEX_ARMV7NEON</location>
        </platform>
        <!-- We need to ask QNAP for platformID for Armv7+ devices (armv7hf) -->
    </item>
